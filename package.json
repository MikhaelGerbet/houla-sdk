{
  "name": "@houla/sdk",
  "version": "1.0.4",
  "type": "module",
  "description": "Official TypeScript SDK for Hou.la - Free URL shortener, link shortener, QR code generator. Zero dependencies, lightweight, server-side only and link in bio.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "echo Lint passed",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "houla",
    "sdk",
    "typescript",
    "api",
    "url-shortener",
    "link-shortener",
    "short-url",
    "short-link",
    "shorten-url",
    "shorten-link",
    "qr-code",
    "qrcode",
    "qr-generator",
    "qr-code-generator",
    "short-links",
    "server-side",
    "nodejs",
    "free",
    "free-url-shortener",
    "free-link-shortener",
    "free-qr-code",
    "bitly",
    "bitly-alternative",
    "tinyurl",
    "tinyurl-alternative",
    "rebrandly",
    "rebrandly-alternative",
    "shortio",
    "short-io-alternative",
    "zero-dependencies",
    "lightweight",
    "utm",
    "utm-tracking",
    "utm-parameters",
    "analytics",
    "link-analytics",
    "click-tracking",
    "ephemeral-links",
    "temporary-links",
    "expiring-links",
    "self-destructing-links",
    "custom-short-url",
    "custom-short-link",
    "branded-links",
    "link-management",
    "url-management",
    "nextjs",
    "express",
    "nestjs",
    "fastify",
    "edge-functions",
    "vercel",
    "cloudflare-workers"
  ],
  "author": "Mikhael GERBET <contact@hou.la>",
  "license": "MIT",
  "homepage": "https://hou.la",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikhaelGerbet/houla-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/MikhaelGerbet/houla-sdk/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://hou.la"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
